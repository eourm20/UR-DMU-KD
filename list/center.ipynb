   "source": [
    "path = 'UCF_Train.list'\n",
    "center = []\n",
    "f = open(path, 'r')\n",
    "for line in f:\n",
    "    if \"__\" in line:\n",
    "        continue\n",
    "    else:\n",
    "        center.append(line)\n",
    "f.close()\n",
    "\n",
    "save_path = 'UCF_Train_center.list'\n",
    "f = open(save_path, 'w')\n",
    "for c in center:\n",
    "    f.write(c)\n",
    "f.close()"
   ]
  }
